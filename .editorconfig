root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = space
indent_size = 4

[*.{fs,fsx,fsi}]
indent_size = 4

[*.{yml,yaml}]
indent_size = 2

[*.{csproj,fsproj,slnx,props,targets}]
indent_size = 2

[Makefile]
indent_style = tab
